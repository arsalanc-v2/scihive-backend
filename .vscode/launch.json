  "version": "0.2.0",
  "configurations": [
    {
      "name": "Fetch Arxiv",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "src/app.py",
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "0"
      },
      "args": ["fetch-arxiv"]
    },
    {
      "name": "Run",
      "type": "python",
      "request": "launch",
      "module": "src.app",
      "console": "integratedTerminal",
      "env": {
        "ENV_FILE": ".local_env"
      }
    },
    {
      "name": "Fetch Twitter",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "src/app.py",
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "0"
      },
      "args": ["fetch-twitter"]
    },
    {
      "name": "Fetch PapersWithCode",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "src/app.py",
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "0"
      },
      "args": ["fetch-paperswithcode"]
    },
    {
      "name": "Migrate DB",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "src/app.py",
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "0"
      },
      "args": ["migrate-db", "--path", "../backup"]
    },
    {
      "name": "Fix stars",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "src/app.py",
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "0"
      },
      "args": ["fix-stars-count"]
    }
  ]
}
